{
  "name": "elastic-ts",
  "version": "0.7.0",
  "description": "TypeScript + Elasticsearch (types, query builder, magic)",
  "repository": "git://github.com/jacobwgillespie/elastic-ts.git",
  "homepage": "https://github.com/jacobwgillespie/elastic-ts",
  "author": "Jacob Gillespie <jacobwgillespie@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "elasticsearch"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "fmt": "prettier --write './**/*.ts'",
    "lint": "prettier --list-different './**/*.ts'",
    "prepack": "rm -rf dist && rollup -c",
    "test": "jest",
    "type-check": "tsc",
    "type-check:tests": "tsc -p tsconfig.test.json",
    "watch": "rollup -c -w"
  },
  "dependencies": {
    "@sindresorhus/is": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^2.0.0",
    "prettier-plugin-pkg": "^0.9.0",
    "rollup": "^2.28.2",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "sideEffects": false,
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "source": "src/index.ts"
}
